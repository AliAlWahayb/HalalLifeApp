{
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "plugin:react/recommended"],
    "rules": {
      "@typescript-eslint/naming-convention": [
        "error",
        {
          "selector": "variableLike",
          "format": ["PascalCase"],
          "leadingUnderscore": "allow"
        }
      ]
    }
  }
  